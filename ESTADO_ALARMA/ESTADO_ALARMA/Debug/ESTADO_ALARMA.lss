000025 e007                          ldi r16, 0x07                    ; Pull-ups para los 3 botones
000026 b908                          out PORTC, r16
                                     
                                     ; Inicializar variables de hora actual
000027 e000                          ldi r16, 0
000028 9300 0100                     sts contador_segundos, r16
00002a 9300 0101                     sts contador_minutos, r16
00002c 9300 0102                     sts contador_horas, r16
00002e 9300 0103                     sts contador_unidades_min, r16
000030 9300 0104                     sts contador_decenas_min, r16
